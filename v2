#!/bin/sh
PYTHONMALLOC=malloc valgrind -v --num-callers=40 --track-fds=yes --leak-check=full --show-leak-kinds=all --track-origins=yes --gen-suppressions=all --suppressions=valgrind.supp --suppressions=/usr/lib/valgrind/debian.supp --suppressions=/usr/lib/valgrind/python3.supp --suppressions=/usr/libexec/valgrind/default.supp --suppressions=/usr/lib/valgrind/python3.supp --suppressions=valgrind-python.supp ./RealmsCode -v
